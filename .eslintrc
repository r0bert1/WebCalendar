{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "consistent-return": "off"
  }
}
